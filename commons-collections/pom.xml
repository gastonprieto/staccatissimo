<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>net.sf.staccato</groupId>
	<artifactId>commons-collections</artifactId>
	<name>Staccato-Commons-Collections</name>
	<version>1.0-SNAPSHOT</version>
	<inceptionYear>2010</inceptionYear>
	<url>http://staccatocommons.sourceforge.net/staccatissimo/commonscollections</url>
	<parent>
		<groupId>net.sf.staccato</groupId>
		<artifactId>commons-parent-pom</artifactId>
		<version>1.0-SNAPSHOT</version>
	</parent>
	<scm>
	   <developerConnection>scm:svn:https://staccatocommons.svn.sourceforge.net/svnroot/staccatocommons/staccatissimo/trunk/commons-collections</developerConnection>
	   <url>http://staccatocommons.svn.sourceforge.net/viewvc/staccatocommons/staccatissimo/trunk/commons-collections/</url>
	</scm>
	<distributionManagement>
		<site>
			<id> ${distribution.site.id}</id>
			<url>${distribution.site.url}/staccatissimo/commonscollections</url>
		</site>
	</distributionManagement>
	<build>
	<plugins>
			<plugin>
				<groupId>net.sf.staccato</groupId>
				<artifactId>commons-check-instrument-maven-plugin</artifactId>
				<version>1.0-SNAPSHOT</version>
				<configuration>
				</configuration>
				<executions>
					<execution>
						<phase>process-classes</phase>
						<goals>
							<goal>instrument</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins> 
	</build>
	<dependencies>
		<dependency>
			<groupId>net.sf.staccato</groupId>
			<artifactId>commons-lang</artifactId>
			<version>1.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>commons-jxpath</groupId>
			<artifactId>commons-jxpath</artifactId>
			<version>1.3</version>
		</dependency>
	</dependencies>
	<reporting>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>findbugs-maven-plugin</artifactId>
                <version>2.0.1</version>
            </plugin>
        </plugins>
    </reporting>
</project>
